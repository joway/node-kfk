{
  "name": "kfk",
  "version": "0.4.0",
  "description": "The high-level node kafka client based on node-rdkafka  .",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT License",
  "repository": {
    "type": "git",
    "url": "https://github.com/joway/node-kfk.git"
  },
  "keywords": [
    "nodejs",
    "kafka",
    "librdkafka",
    "node-rdkafka",
    "high-level"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "purge": "rm -rf package-lock.json node_modules && npm run clean",
    "build": "npm run clean && npm run compile",
    "lint": "tslint src/**/*.ts --project .",
    "test": "NODE_ENV=ci nyc ava dist/tests -s -v",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "prettier": "prettier --config ./.prettierrc --write '{src,tests}/**/*.ts'"
  },
  "author": "joway",
  "peerDependencies": {
    "node-rdkafka": "^2.8.1"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "lodash": "^4.17.15",
    "moment": "2.29.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.33",
    "@types/lodash": "4.14.168",
    "@types/node": "^14.0.1",
    "@types/sinon": "9.0.11",
    "@types/supertest": "2.0.10",
    "ava": "1.4.0",
    "coveralls": "^3.0.2",
    "node-rdkafka": "^2.8.1",
    "nodemon": "2.0.7",
    "nyc": "^15.0.0",
    "sinon": "9.2.4",
    "supertest": "6.1.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "3.9.9"
  }
}
