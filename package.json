{
  "name": "kfk",
  "version": "0.3.2",
  "description": "The high-level node kafka client based on node-rdkafka  .",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT License",
  "repository": {
    "type": "git",
    "url": "https://github.com/joway/node-kfk.git"
  },
  "keywords": [
    "nodejs",
    "kafka",
    "librdkafka",
    "node-rdkafka",
    "high-level"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "purge": "rm -rf package-lock.json node_modules && npm run clean",
    "build": "npm run clean && npm run compile",
    "lint": "tslint src/**/*.ts --project .",
    "test": "NODE_ENV=ci nyc ava dist/tests -s -v",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "prettier": "prettier --config ./.prettierrc --write '{src,tests}/**/*.ts'"
  },
  "author": "joway",
  "dependencies": {
    "bluebird": "3.5.3",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "node-rdkafka": "2.5.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.26",
    "@types/lodash": "4.14.123",
    "@types/node": "^11.9.2",
    "@types/sinon": "7.0.9",
    "@types/supertest": "2.0.7",
    "ava": "1.3.1",
    "coveralls": "^3.0.2",
    "nodemon": "1.18.10",
    "nyc": "^13.0.1",
    "sinon": "7.2.7",
    "supertest": "4.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "3.3.3333"
  }
}
